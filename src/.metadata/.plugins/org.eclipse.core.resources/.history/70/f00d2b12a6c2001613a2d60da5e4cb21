package wu.leizi.moniterConfig;

import java.util.ArrayList;
import java.util.Properties;

import kafka.consumer.ConsumerConfig;

public class configContent {
	private double HeartBeatInterval;
	private double TimeOutInterval;
	private ConsumerConfig Consumerconfig;
	private static configContent instance = null;
	private configContent() {
		HeartBeatInterval = 300;
		TimeOutInterval = 1000;
		
	}
	public configContent getInstance() {
		if (instance == null) instance = new configContent();
		return instance;
	}
	public double getHeartBeatInterval() {
		return HeartBeatInterval;
	}
	public double getTimeOutInterval() {
		return TimeOutInterval;
	}

	public boolean refresh(){
		return true;
	}
